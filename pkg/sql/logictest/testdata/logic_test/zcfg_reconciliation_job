# Ensure there's a single ZCFG RECONCILIATION job in the system and it's running.
query ITIT colnames
SELECT count(*), job_type, job_id, status FROM [SHOW JOBS] WHERE job_type ='ZCFG RECONCILIATION' GROUP BY(job_id, job_type, status)
----
count  job_type             job_id  status
1      ZCFG RECONCILIATION  1       running

statement error job 1: not cancelable
CANCEL JOB 1

# The job should still be running.
query IT colnames
SELECT job_id, status FROM [SHOW JOBS] WHERE job_type ='ZCFG RECONCILIATION'
----
job_id  status
1       running
